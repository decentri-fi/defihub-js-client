{
  "name": "@defitrack/js-client",
  "version": "1.0.0",
  "description": "Client-side js library for easy use of the Defitrack Defi API",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/defitrack/js-api.git"
  },
  "keywords": [
    "Defi",
    "Web3",
    "API",
    "SDK"
  ],
  "author": "Money Lisa <themoneylisa@proton.me> (https://defitrack.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/defitrack/js-api/issues"
  },
  "homepage": "https://docs.defitrack.io",
  "dependencies": {
    "@types/node": "^12.7.1",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/big.js": "^6.1.3",
    "babel-loader": "^8.2.5",
    "big.js": "^5.2.2",
    "prettier": "^2.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  }
}
